{
  "name": "build-checker-app",
  "version": "0.0.1",
  "main": "bootstrapper.js",
  "productName": "BuildCheckerApp",
  "identifier": "com.example.build-checker-app",
  "license": "MIT",
  "repository": "https://github.com/willmendesneto/build-checker-app",
  "scripts": {
    "start": "gulp serve",
    "build": "export NODE_ENV='production'; gulp build && build-electronjs",
    "build-electronjs": "gulp build && electron-packager build BuildCheckerApp --platform=all --cache=cache --arch=all --overwrite --out=releases --ignore='^/(bower_components|dist|resources|tasks|cache|sass)$' --prune --version=$npm_package_electronVersion",
    "build-electronjs-asar": "gulp build && electron-packager build BuildCheckerApp --platform=all --cache=cache --arch=all --overwrite --out=releases --ignore='^/(bower_components|dist|resources|tasks|cache|sass)$' --prune --asar --version=$npm_package_electronVersion",
    "build:asar": "export NODE_ENV='production'; gulp build && build-electronjs-asar"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "bluebird": "^3.1.5",
    "history": "^1.17.0",
    "jquery": "^2.2.0",
    "node-notifier": "^4.4.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-router": "^2.0.0-rc5",
    "request": "^2.69.0"
  },
  "devDependencies": {
    "appdmg": "^0.3.6",
    "bower": "^1.7.7",
    "browserify": "^13.0.0",
    "del": "^2.2.0",
    "electron-connect": "^0.3.0",
    "electron-debug": "^0.5.1",
    "electron-ipc-stream": "^0.1.0",
    "electron-packager": "^5.2.1",
    "electron-prebuilt": "^0.36.0",
    "fs-jetpack": "^0.7.1",
    "gulp": "^3.8.11",
    "gulp-babel": "^6.1.1",
    "gulp-concat": "^2.6.0",
    "gulp-flatten": "0.2.0",
    "gulp-if": "^2.0.0",
    "gulp-image-optimization": "^0.1.3",
    "gulp-imagemin": "^2.4.0",
    "gulp-inject": "^3.0.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-minify-css": "^1.1.6",
    "gulp-minify-html": "^1.0.5",
    "gulp-plumber": "^1.0.1",
    "gulp-sass": "^2.0.1",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-task-listing": "^1.0.1",
    "gulp-uglify": "^1.2.0",
    "gulp-useref": "^3.0.4",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.2.4",
    "lodash": "^4.0.0",
    "main-bower-files": "^2.8.0",
    "merge2": "^0.3.5",
    "mkdirp": "^0.5.1",
    "optimist": "^0.6.1",
    "q": "^1.4.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "xo": "^0.12.1",
    "yargs": "^4.1.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "browser"
    ]
  },
  "engine": "node >= 0.12"
}
