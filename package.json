{
  "name": "electron-es6-react",
  "version": "0.0.1",
  "main": "bootstrapper.js",
  "license": "MIT",
  "repository": "https://github.com/willmendesneto/build-checker-app",
  "scripts": {
    "start": "gulp serve",
    "build": "export NODE_ENV='production'; gulp bower:install && gulp inject:css && gulp clean && electron-packager . $npm_package_productName --platform=darwin --arch=x64 --overwrite --out=dist --ignore='^/dist$' --prune --version=$npm_package_electronVersion",
    "build:asar": "export NODE_ENV='production'; gulp bower:install && gulp inject:css && gulp clean && electron-packager . $npm_package_productName --platform=darwin --arch=x64 --overwrite --out=dist --ignore='^/dist$' --prune --asar --version=$npm_package_electronVersion"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.3.13",
