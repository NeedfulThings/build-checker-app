    "history": "^1.17.0",
    "jquery": "^2.2.0",
    "node-notifier": "^4.4.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-router": "^2.0.0-rc5",
    "request": "^2.69.0"
  },
  "devDependencies": {
    "bower": "^1.7.7",
    "browserify": "^13.0.0",
    "del": "^2.2.0",
    "electron-connect": "^0.3.0",
    "electron-debug": "^0.5.1",
